[
  {
    "name": "runningdinner-backend",
    "image": "clemensstich/runningdinner:${IMAGE_TAG_VERSION}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "runningdinner-backend",
        "awslogs-region": "${AWS_REGION}",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "links": [],
    "portMappings": [
      {
        "hostPort": 80,
        "containerPort": 80,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "entryPoint": [],
    "command": [],
    "environment": [
      {
        "name": "SENDGRID.API.KEY_TODO",
        "value": "TODO"
      }
    ],
    "secrets": [
      {
        "name": "SPRING_DATASOURCE_URL",
        "valueFrom": "${SPRING_DATASOURCE_URL_SSM_ARN}"
      },
      {
        "name": "SPRING_DATASOURCE_USERNAME",
        "valueFrom": "${SPRING_DATASOURCE_USERNAME_SSM_ARN}"
      },
      {
        "name": "SPRING_DATASOURCE_PASSWORD",
        "valueFrom": "${SPRING_DATASOURCE_PASSWORD_SSM_ARN}"
      },
      {
        "name": "SPRING_FLYWAY_USER",
        "valueFrom": "${SPRING_FLYWAY_USER_SSM_ARN}"
      },
      {
        "name": "SPRING_FLYWAY_PASSWORD",
        "valueFrom": "${SPRING_FLYWAY_PASSWORD_SSM_ARN}"
      },
      {
        "name": "AWS_SQS_GEOCODE_URL",
        "valueFrom": "${AWS_SQS_GEOCODE_URL_SSM_ARN}"
      }
    ],
    "volumes": [
      {
        "name": "app-logs",
        "host": {
          "sourcePath": "/logs"
        }
      }
    ],
    "mountPoints": [
      {
        "containerPath": "/logs",
        "sourceVolume": "app-logs"
      }
    ],
    "volumesFrom": []
  }
]